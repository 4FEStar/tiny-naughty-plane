        let svg = Snap('#mysvg');
        let b1 = [];
        document.getElementById("button").onclick = function () {
            for (let i = 0; i < 1; i++)
                b1[i] = test();
        }
        function test() {
            let b1 = new threeBullet(svg, Math.ceil(Math.random() * 1000 + 30), Math.ceil(Math.random() * 700 + 30), {
                x: 1,
                y: 1.5
            });
            b1.move();

            setTimeout(() => {
                b1.booom();
            }, Math.ceil(Math.random() * 1000 + 500));
            return b1;
        };
